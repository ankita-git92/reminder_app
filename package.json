    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@react-native-community/viewpager": "^5.0.11",
    "babel-jest": "^26.6.3",
