{
  "name": "altvest",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "yarn && yarn postinstall && react-native start",
    "dev": "yarn start",
    "test": "jest",
    "lint": "eslint .",
    "generate:debug:apk": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/ && cd android && ./gradlew assembleDebug",
    "g:d:a": "yarn generate:debug:apk",
    "g:a": "yarn generate:debug:apk && mv android/app/build/outputs/apk/debug/app-debug.apk .",
    "stage:start": "pm2 start stage-server-pm2.json",
    "stage:stop": "pm2 delete stage-server-pm2.json",
    "postinstall": "patch-package",
    "release:ios:beta": "yarn && cd ios/ && pod install && fastlane beta" 
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.15.13",
    "@react-navigation/bottom-tabs": "^6.0.9",''
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/native-stack": "^6.2.5",
    "@reduxjs/toolkit": "^1.7.1",
    "@types/react-native": "^0.66.4",
    "@types/styled-components": "^5.1.15",
    "accordion-collapse-react-native": "^1.1.0",
    "axios": "^0.24.0",
    "dotenv": "^10.0.0",
    "formik": "^2.2.9",
    "jwt-decode": "^3.1.2",
    "mime": "^3.0.0",
    "monent": "^0.0.2-security",
    "patch-package": "^6.4.7",
    "pm2": "^5.1.2",
    "postinstall-postinstall": "^2.1.0",
    "react": "17.0.2",
    "react-native": "0.66.3",
    "react-native-check-box": "^2.1.7",
    "react-native-date-picker": "^4.1.3",
    "react-native-document-picker": "^8.0.0",
    "react-native-dotenv": "^3.3.0",
    "react-native-dropdown-picker": "^5.3.0",
    "react-native-gifted-chat": "^0.16.3",
    "react-native-material-menu": "^2.0.0",
    "react-native-multi-selectbox": "^1.5.0",
    "react-native-multiple-select": "^0.5.7",
    "react-native-pager-view": "^5.4.9",
    "react-native-responsive-screen": "^1.4.2",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.9.0",
    "react-native-simple-download-manager": "^1.4.1",
    "react-native-svg": "^12.1.1",
    "react-native-tab-view": "^3.1.1",
    "react-native-vector-icons": "^9.0.0",
    "react-native-video": "^5.2.0",
    "react-native-video-player": "^0.12.0",
    "react-native-webview": "^11.15.0",
    "react-redux": "^7.2.6",
    "redux": "^4.1.2",
    "redux-thunk": "^2.4.0",
    "rn-fetch-blob": "^0.12.0",
    "socket.io-client": "^4.1.3",
    "styled-components": "^5.3.3",
    "yup": "^0.32.11"
  },
